tifDir = dir('*_maxCorrProject.tif');
ROI = struct;
for i = 1:length(tifDir)
    batName = tifDir(i).name(1:3);
    dateSesh = tifDir(i).name(5:10);
    
    ROI(i).ROIname = FS_annotate_image(tifDir(i).name);
end


